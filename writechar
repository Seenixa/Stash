object writechar{
  def main(args: Array[String]){
  
    // Irjunk fuggvenyt, mely parameterkent egy karaktert es egy egesz erteket kap, es ennyi darabot ir ki egymas utan az atadott karakterbol! //

    val number = 0
    val letter = 'a'
    
    for( i <- 0 until number)
      print(" " + letter)
  }
}
